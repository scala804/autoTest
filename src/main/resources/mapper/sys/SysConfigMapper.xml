<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.scala.autoTest.dao.sys.SysConfigDao">

  <select id="selectByPrimaryKey" parameterType="java.lang.String" resultType="Integer">
    select count(1)
    from sys_config
    where variable = #{variable}
  </select>

</mapper>